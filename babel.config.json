{
    "ignore": ["node_modules/**"],
    "presets": [
        "@babel/preset-typescript",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "14"
                },
                "useBuiltIns": "usage",
                "corejs": "3.6.5",
                "modules": "cjs"
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-modules-commonjs",
            {
                "importInterop": "node"
            }
        ]
    ]
}